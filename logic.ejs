<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Logic</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
  </head>

  <body>

    <div class="container">
      <h1>Shows:</h1>
      <div class="row">
	<div class="list-group" style="width: 75%">
	  <% var dataParsed = JSON.parse(data);
	     for (var i = 0; i < dataParsed.length; i++) { %>
	     <a class="list-group-item">
	       <%= dataParsed[i]["artist_name"] %> in <%= dataParsed[i]["place"] %>, author id: <%= dataParsed[i]["author_id"] %>, timestamp: <%= dataParsed[i]["creation_time"] %>, approved by moderators: <%= dataParsed[i]["is_approved"] %>
	     </a>
	  <% } %>
	</div>
      </div>
      <div class="row">
	<a class="btn btn-primary" href="/new">Add a new show</a>
      </div>
    </div>

  </body>
</html>
